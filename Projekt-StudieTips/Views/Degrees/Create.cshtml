@model Projekt_StudieTips.Models.Degree

@{
    ViewData["Title"] = "Create";
}

<h1>Opret uddannelse</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Uddannelse</label>
                <input asp-for="DegreeName" class="form-control" />
                <span asp-validation-for="DegreeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Opret" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Tilbage til uddannelse</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
